    "@commitlint/config-conventional": "^18.4.3",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^12.1.5",
    "@testing-library/react-hooks": "^8.0.0",
    "@types/lodash.get": "^4.4.7",
    "@types/lodash.set": "^4.3.7",
    "@types/react": "17",
    "c8": "^8.0.1",
    "graphql": "^16.5.0",
    "husky": "^8.0.1",
    "jsdom": "^23.0.1",
    "prettier": "^3.1.1",
    "react": "17",
    "react-dom": "17",
    "react-test-renderer": "17",
    "ts-toolbelt": "^9.6.0",
    "tsup": "^8.0.1",
