{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "@ofrbg/pqrs",
  "version": "0.0.2",
  "description": "Query-less GraphQL",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OFRBG/pqrs.git"
  },
  "bugs": {
    "url": "https://github.com/OFRBG/pqrs/issues"
  },
  "homepage": "https://github.com/OFRBG/pqrs#readme",
  "author": "Oscar Fonseca <oscar.fonseca@formidable.com>",
  "license": "MIT",
  "type": "module",
  "publishConfig": {
    "@OFRBG:registry": "https://npm.pkg.github.com"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "exports": {
    "require": "./dist/index.js",
    "import": "./dist/index.mjs"
  },
  "types": "./dist/index.d.ts",
  "scripts": {
    "build-fast": "tsup src/index.ts --format cjs,esm",
    "build": "yarn build-fast --dts-resolve",
    "test": "vitest",
    "prepublishOnly": "yarn build"
  },
  "dependencies": {
    "lodash.get": "^4.4.2",
    "lodash.set": "^4.3.2"
  },
  "peerDependencies": {
    "react": "17 || 18"
  },
  "devDependencies": {
    "@testing-library/react-hooks": "^8.0.0",
    "@tsconfig/esm": "^1.0.0",
    "@types/lodash.get": "^4.4.7",
    "@types/lodash.set": "^4.3.7",
    "@types/prettier": "^2.6.3",
    "@types/react": "^18.0.12",
    "prettier": "^2.6.2",
    "react-test-renderer": "^18.1.0",
    "ts-toolbelt": "^9.6.0",
    "tsup": "^6.1.2",
    "typescript": "^4.7.3",
    "vitest": "^0.14.2"
  }
}
